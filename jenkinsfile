pipeline {
    agent any
    stages{
        stage ('build') {
            steps {
            echo 'hi im building'
            }
            
        }
        stage ('test') {
            steps {
            echo 'im testing'     
            }
            
        }
        stage ('deploy') {
            steps {
             echo 'im deployinf for the first time'
            }
            
        }  
    }
}


pipeline {
    agent any
    stages{
        stage ('perf') {
            steps {
            echo 'im practising'
            }
        
        } 
         stage ('build') {
            steps {
            echo ('this is also practise one')
            }

         }
        stage ('test')
           steps {
           echo ('i have to check')
           }
           
    }
}