pipeline{
    agent any
    stages {
        stage ('build') {        
          steps {
              echo 'it is build step'
              sh 'mvn -B -DskipTests clean package' 
          }
              
          }  
          stage ('test') {
              steps {
                  sh 'mvn test'
              }
          } 
            

          } 
          }      
        
